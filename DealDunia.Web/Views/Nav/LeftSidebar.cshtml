@model IEnumerable<string>
<h5><a href="@Url.Action("Daily", "Deal")">Daily Deals</a></h5>
<h5><a href="@Url.Action("Exclusive", "Deal")">Exclusive Deals</a></h5>
<h5><a href="@Url.Action("Stores", "Store")">Online Stores</a></h5>

<h5>Coupons & Promotions</h5>
<ul class="list-group">
    @foreach (var coupon in Model)
    {
        <li class="list-group-item"><a href="@Url.RouteUrl("CouponsByCategory", new { Category = @coupon })">@coupon</a></li>
    }
</ul>