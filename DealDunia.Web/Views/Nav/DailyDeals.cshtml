@model IEnumerable<DealDunia.Domain.Entities.DailyDeals>

<div class="row">
    @foreach (var deal in Model)
    {
        <div class="jumbotron col-md-4 col-lg-4 text-center">
            <img  src="@Url.Content("~/Images/" + deal.Image)" alt="@deal.Title" width="100" height="100"/>
            <br />
            <a href="@deal.URL"><b>@deal.Title</b></a>
        </div>
    }
</div>
