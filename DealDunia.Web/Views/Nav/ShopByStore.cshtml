@model IEnumerable<DealDunia.Domain.Entities.Store>

<div class="row">
    @foreach (var store in Model)
    {
        <div class="text-center" style="float:left;margin-right:40px;margin-bottom:20px;">
            <img  src="@Url.Content("~/Images/" + store.StoreImage)" alt="@store.StoreName" width="100" height="50"/>
            <br />
            <b><a href="@Url.RouteUrl("Store", new { store = store.StoreName})">@store.StoreName</a></b>
        </div>
    }
</div>
