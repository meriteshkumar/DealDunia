@model IEnumerable<DealDunia.Domain.Entities.Store>

<div class="row">
    @foreach (var store in Model)
    {
        <div class="jumbotron col-md-4 col-lg-4 text-center">
            <img  src="@Url.Content("~/Images/" + store.StoreImage)" alt="@store.StoreName" width="100" height="100"/>
            <br />
            <b><a href="@Url.RouteUrl("Store", new { store = store.StoreName})">@store.StoreName</a></b>
        </div>
    }
</div>
