@model IEnumerable<DealDunia.Domain.Entities.Category>
@using DealDunia.Infrastructure.Utility

<div class="row">
    @foreach (var category in Model)
    {
        <div class="text-center" style="float:left;margin-right:40px;margin-bottom:20px;">
            <a href="@Url.RouteUrl("Level1", new { id1 = Utilities.EncodeUrl(category.CategoryName) })">
                <img src="@Url.Content("~/Images/" + category.Image)" alt="@category.CategoryName" /></a>
            <br />
            <span><a href="@Url.RouteUrl("Level1", new { id1 = Utilities.EncodeUrl(category.CategoryName) })"><strong>@category.CategoryName</strong></a></span>
        </div>
    }
</div>
